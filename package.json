{
  "name": "groupread",
  "version": "0.0.1",
  "description": "Self-hosted book club platform",
  "main": "index.js",
  "scripts": {
    "start": "node build/server/index.js",
    "dev:server": "tsc-watch --project server/tsconfig.json --onSuccess \"npm start\"",
    "dev:ui": "webpack serve --mode=development",
    "test:jest": "NODE_ENV=test jest --config ./server/jest.config.ts --verbose --runInBand",
    "start:cypress": "NODE_ENV=test npm start",
    "test:e2e": "cypress run",
    "lint": "eslint .",
    "analyze": "source-map-explorer 'build/frontend/main.js'",
    "build:server": "tsc --build server/tsconfig.json",
    "build:ui": "webpack --mode=production",
    "build": "npm run build:server && npm run build:ui",
    "start:full": "npm run build && npm start"
  },
  "author": "Camden Mecklem",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@hookform/error-message": "2.0.0",
    "@reduxjs/toolkit": "^1.6.2",
    "assert": "^2.0.0",
    "axios": "^0.24.0",
    "bcrypt": "^5.0.1",
    "buffer": "^6.0.3",
    "bulma": "^0.9.3",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dayjs": "^1.10.7",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "react": "^17.0.1",
    "react-bootstrap-icons": "^1.6.1",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.19.5",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.0.2",
    "redux": "^4.1.2",
    "redux-thunk": "^2.4.0",
    "remark": "^14.0.2",
    "remark-html": "^15.0.0",
    "sequelize": "^6.9.0",
    "styled-components": "^5.3.3",
    "title-case": "^3.0.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.0.2",
    "@types/jsonwebtoken": "^8.5.6",
    "@types/mini-css-extract-plugin": "^2.4.0",
    "@types/node": "^16.11.10",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-redux": "^7.1.20",
    "@types/styled-components": "^5.1.15",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.3",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.4.0",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "cypress": "^9.0.0",
    "eslint": "^8.2.0",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-react": "^7.27.0",
    "html-webpack-plugin": "^5.5.0",
