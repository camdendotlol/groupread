{
  "name": "groupread",
  "version": "0.0.1",
  "description": "Self-hosted book club platform",
  "main": "index.js",
  "scripts": {
    "start": "node build/server/index.js",
    "dev:server": "tsc-watch --project server/tsconfig.json --onSuccess \"npm start\"",
    "dev:ui": "webpack serve --mode=development",
    "test:jest": "NODE_ENV=test jest --config ./server/jest.config.ts --verbose --runInBand",
    "start:cypress": "NODE_ENV=test npm start",
    "test:e2e": "cypress run",
    "lint": "eslint .",
    "analyze": "source-map-explorer 'build/frontend/main.js'",
    "build:server": "tsc --build server/tsconfig.json",
    "build:ui": "webpack --mode=production",
    "build": "npm run build:server && npm run build:ui",
    "start:full": "npm run build && npm start"
  },
  "author": "Camden Mecklem",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@hookform/error-message": "2.0.0",
    "@reduxjs/toolkit": "^1.6.1",
    "assert": "^2.0.0",
    "axios": "^0.22.0",
    "bcrypt": "^5.0.1",
    "bootstrap-icons": "^1.5.0",
    "bulma": "^0.9.3",
    "chalk": "^4.1.2",
    "cors": "^2.8.5",
    "dayjs": "^1.10.6",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^7.17.0",
    "react-redux": "^7.2.5",
    "react-router-dom": "^5.2.1",
    "redux": "^4.1.1",
    "redux-thunk": "^2.3.0",
    "remark": "^14.0.1",
    "remark-html": "^15.0.0",
    "sequelize": "^6.6.5",
    "styled-components": "^5.3.0",
    "title-case": "^3.0.3",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.14.5",
    "@cypress/react": "^5.9.4",
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^27.0.1",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mini-css-extract-plugin": "^2.3.0",
    "@types/node": "^16.3.2",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.3",
    "@types/react-redux": "^7.1.18",
    "@types/react-router-dom": "^5.1.9",
    "@types/styled-components": "^5.1.11",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.33.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "cypress": "^8.5.0",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
